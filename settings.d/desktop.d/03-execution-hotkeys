### Simple utils
bindsym XF86Calculator exec qalculate
bindsym Print exec maim ~/workshop/scrots/$(date +%m-%d_%H%S).png
bindsym Shift+Print exec maim ~/workshop/scrots/$(date +%m-%d_%H%S)_select.png -s
bindsym $mod+Shift+a exec /home/nick/.bin/toggle_loopback
# Centered version of the above. A E S T H E T I C
bindsym $mod+y fullscreen disable; floating enable; resize set 1272 720; move position center; border pixel 0;

### Browser Crap
bindsym $mod+slash exec firefox-esr
bindsym $mod+Shift+slash exec firefox-esr --private-window

